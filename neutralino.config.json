{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.cjs.whatsapp",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/cjs-whatsapp-ui/build/",
  "url": "/",
  "port": 55337,
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "exportAuthInfo": true,
  "enableExtensions": true,
  "extensions": [
    {
      "id": "js.neutralino.cjs.whatsapp.extension",
      "commandWindows": "${NL_PATH}/extensions/cjs-whatsapp-ext/cjs-whatsapp-ext.exe"
    }
  ],
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "window.*",
    "debug.log",
    "filesystem.*",
    "extensions.*"
  ],
  "globalVariables": {
  },
  "modes": {
    "window": {
      "title": "Whatsapp Web Messaging",
      "width": 1200,
      "height": 1200,
      "minWidth": 800,
      "minHeight": 800,
      "enableInspector": true,
      "icon": "/cjs-whatsapp-ui/public/logo192.png",
      "exitProcessOnClose": true,
      "borderless": false
    }
  },
  "cli": {
    "binaryName": "cjs-whatsapp",
    "resourcesPath": "/cjs-whatsapp-ui/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/cjs-whatsapp-ui/public/__neutralino_globals.js",
    "binaryVersion": "4.11.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/cjs-whatsapp-ui/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}